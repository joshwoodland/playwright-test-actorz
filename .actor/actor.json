{
	"actorSpecification": 1,
	"name": "playwright-test",
	"version": "1.0.0",
	"buildTag": "latest",
	"dockerfile": "./Dockerfile",
	"input": "./INPUT_SCHEMA.json",
	"defaultRunOptions": {
		"build": {
			"memory": 8192
		},
		"run": {
			"memoryMbytes": 8192,
			"cpu": 2
		}
	},
	"environmentVariables": {
		"EMAIL": {
			"name": "Email",
			"description": "Email address for login",
			"type": "string",
			"required": true,
			"secret": true
		},
		"PASSWORD": {
			"name": "Password",
			"description": "Password for login",
			"type": "string",
			"required": true,
			"secret": true
		}
	},
	"storages": {
        "dataset": "./OUTPUT_SCHEMA.json"
    }
}
